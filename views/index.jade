extends layout

block content
	h1= title
	p Welcome to #{title}

	form(id='sign-up-form')
		input(type='input', id='username')
		input(type='input', id='first-name')
		input(type='input', id='last-name')
		input(type='password', id='password')
		input(type='password', id='confirm-password')
		input(type='email', id='email')
		button(id='sign-up-button') Sign Up